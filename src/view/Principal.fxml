<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>

<MenuBar xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PrincipalController">
  <menus>
    <Menu mnemonicParsing="false" text="Menu">
      <items>
        <MenuItem fx:id="btnSender" mnemonicParsing="false" onAction="#btnSenderOnAction" text="Sender" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="btnReceiver" mnemonicParsing="false" onAction="#btnReceiverOnAction" text="Receiver" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="btnSair" onAction="#sairOnAction" text="Sair" />
      </items>
    </Menu>
  </menus>
</MenuBar>
